{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "declaration": true,
    "experimentalDecorators": true,
    "outDir": "./dist/",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": true
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
